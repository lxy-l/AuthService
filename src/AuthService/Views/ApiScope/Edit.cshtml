@{
    ViewData["Title"] = "Edit ApiScope";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using IdentityServer7.EntityFramework.Storage.Entities;
@model ApiScope
<h1>Edit</h1>
<hr />
<p>
    <a asp-action="Index">Back to list</a>
</p>
<div class="row">
    <form id="registerForm" method="post">
        <div asp-validation-summary="ModelOnly" class="text-danger" role="alert"></div>
        <div class="form-floating mb-3">
            <input asp-for="Name" class="form-control" disabled autocomplete="Name" aria-required="true" />
            <label asp-for="Name">Name</label>
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-floating mb-3">
            <input asp-for="DisplayName" class="form-control" disabled autocomplete="DisplayName" aria-required="true" />
            <label asp-for="DisplayName">DisplayName</label>
            <span asp-validation-for="DisplayName" class="text-danger"></span>
        </div>
        <div class="form-floating mb-3">
            <input asp-for="Description" class="form-control" disabled autocomplete="Description" aria-required="true" />
            <label asp-for="Description">Description</label>
            <span asp-validation-for="Description" class="text-danger"></span>
        </div>
        <div class="form-floating mb-3">
            <input asp-for="Enabled" class="form-control" disabled autocomplete="Enabled" aria-required="true" />
            <label asp-for="Enabled">Enabled</label>
            <span asp-validation-for="Enabled" class="text-danger"></span>
        </div>
        <button id="registerSubmit" type="submit" class="w-100 btn btn-lg btn-primary">Save</button>
    </form>
</div>
